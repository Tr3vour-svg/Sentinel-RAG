port matplotlib.pyplot as plt\n","import time\n","\n","def run_evaluation_suite(samples):\n","    results = []\n","\n","    for i, sample in enumerate(samples):\n","        print(f\"ğŸ§ª Testing Sample {i+1}/{len(samples)}: {sample['question']}\")\n","\n","        # Run the RAG pipeline\n","        start_time = time.time()\n","        output = rag_pipeline(sample['question'])\n","        latency = time.time() - start_time\n","\n","        # Use an LLM-Judge to score the response (0-10)\n","        # We use a simple prompt to get a numerical score\n","        eval_score_raw = gemini_25_generate(f\"On a scale of 0 to 10, how accurately does this answer: '{output['answer']}' match the ground truth: '{sample['ground_truth']}'? Return ONLY the number.\")\n","\n","        try:\n","            score = float(eval_score_raw.strip())\n","        except:\n","            score = 0.0\n","\n","        # Determine success based on whether the answer is the fallback error message\n","        # This is a proxy since rag_pipeline doesn't return a 'relevant' flag directly\n","        is_successful_answer = \"I'm sorry, I'm currently experiencing high traffic\" not in output['answer']\n","\n","        results.append({\n","            \"question\": sample['question'],\n","            \"score\": score,\n","            \"latency\": latency,\n","            \"success\": is_successful_answer\n","        })\n","\n","        # Small sleep to respect Free Tier RPM limits\n","        time.sleep(4)\n","\n","    # --- Visualization ---\n","    names = [f\"Q{i+1}\" for i in range(len(results))]\n","    scores = [r['score'] for r in results]\n","\n","    plt.figure(figsize=(10, 5))\n","    colors = ['green' if s >= 7 else 'orange' if s >= 4 else 'red' for s in scores]\n","    plt.bar(names, scores, color=colors)\n","    plt.axhline(y=7, color='gray', linestyle='--', label='Target Accuracy')\n","    plt.ylim(0, 10)\n","    plt.ylabel('Accuracy Score (0-10)')\n","    plt.title('RAG Pipeline Performance by Question')\n","    plt.legend()\n","    plt.show()\n","\n","    return results\n","\n","# EXECUTE:\n","final_results = run_evaluation_suite(eval_samples)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"kaGm4ly6wbGz","executionInfo":{"status":"ok","timestamp":1767461848637,"user_tz":-240,"elapsed":463749,"user":{"displayName":"Travour Infinity","userId":"05960736380634090970"}},"outputId":"cf7f6faf-aa8d-457e-9870-6ecb1bcf98b8"},"execution_count":22,"outputs":[{"output_type":"stream","name":"stdout","text":["ğŸ§ª Testing Sample 1/7: In the 2025 10-K, what specific amount did Apple report as the aggregate market value of voting and non-voting stock held by non-affiliates as of March 28, 2025?\n","ğŸ”„ Attempt 1...\n","âŒ Request Failed: 429 RESOURCE_EXHAUSTED. {'error': {'code': 429, 'message': 'You exceeded your current quota, please check your plan and billing details. For more information on this error, head to: https://ai.google.dev/gemini-api/docs/rate-limits. To monitor your current usage, head to: https://ai.dev/usage?tab=rate-limit. \\n* Quota exceeded for metric: generativelanguage.googleapis.com/generate_content_free_tier_requests, limit: 20, model: gemini-2.5-flash\\nPlease retry in 10.268620238s.', 'status': 'RESOURCE_EXHAUSTED', 'details': [{'@type': 'type.googleapis.com/google.rpc.Help', 'links': [{'description': 'Learn more about Gemini API quotas', 'url': 'https://ai.google.dev/gemini-api/docs/rate-limits'}]}, {'@type': 'type.googleapis.com/google.rpc.QuotaFailure', 'violations': [{'quotaMetric': 'generativelanguage.googleapis.com/generate_content_free_tier_requests', 'quotaId': 'GenerateRequestsPerDayPerProjectPerModel-FreeTier', 'quotaDimensions': {'location': 'global', 'model': 'gemini-2.5-flash'}, 'quotaValue': '20'}]}, {'@type': 'type.googleapis.com/google.rpc.RetryInfo', 'retryDelay': '10s'}]}}\n","ğŸ’¡ Tip: If this is a 429 error, wait 1 minute and rerun this cell.\n","âš ï¸ gemini-2.5-flash returned None (l