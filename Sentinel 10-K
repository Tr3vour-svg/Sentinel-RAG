rid_retrieve_traced(query)\n","    answer = generate_answer_traced(query, docs)\n","    return answer"],"metadata":{"id":"NKOcm6lS8d5D","executionInfo":{"status":"ok","timestamp":1767462008278,"user_tz":-240,"elapsed":29,"user":{"displayName":"Travour Infinity","userId":"05960736380634090970"}}},"execution_count":23,"outputs":[]},{"cell_type":"markdown","source":["Calibration Fitting (Temperature Scaling)"],"metadata":{"id":"dUK2vaHzAgmQ"}},{"cell_type":"code","source":["import numpy as np\n","from scipy.optimize import minimize_scalar\n","\n","def calibrate_confidence(logits, labels):\n","    \"\"\"\n","    Fits a temperature 'T' to minimize Negative Log Likelihood.\n","    Used for the 'Critique' model to ensure 'YES/NO' is honest.\n","    \"\"\"\n","    def nll_loss(t):\n","        scaled_logits = logits / t\n","        # Simplified cross-entropy calculation\n","        return -np.mean(labels * scaled_logits - np.log(1 + np.exp(scaled_logits)))\n","\n","    res = minimize_scalar(nll_loss, bounds=(0.1, 5.0), method='bounded')\n","    return res.x # This is your optimal Temperature scalar"],"metadata":{"id":"oK1Ij8GnAeOM","executionInfo":{"status":"ok","timestamp":1767463089618,"user_tz":-240,"elapsed":4,"user":{"displayName":"Travour Infinity","userId":"05960736380634090970"}}},"execution_count":28,"outputs":[]},{"cell_type":"markdown","source":["Streamlit UI"],"metadata":{"id":"fOOL5zF18mNU"}},{"cell_type":"code","source":["!pip install -q streamlit"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"AOcjp6A2_4kY","executionInfo":{"status":"ok","timestamp":1767462910915,"user_tz":-240,"elapsed":8602,"user":{"displayName":"Travour Infinity","userId":"05960736380634090970"}},"outputId":"44c7a60f-653b-4616-9380-99cd342ad697"},"execution_count":25,"outputs":[{"output_type":"stream","name":"stdout","text":["\u001b[2K   \u001b[90mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\u001b[0m \u001b[32m9.0/9.0 MB\u001b[0m \u001b[31m59.3 MB/s\u001b[0m eta \u001b[36m0:00:00\u001b[0m\n","\u001b[2K   \u001b[90mâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\u001b[0m \u001b[32m6.9/6.9 MB\u001b[0m \u001b[31m75.1 MB/s\u001b[0m eta \u001b[36m0:00:00\u001b[0m\n","\u001b[?25h"]}]},{"cell_type":"code","source":["import numpy as np\n","import streamlit as st\n","\n","def get_calibrated_confidence(raw_score, temperature=1.2):\n","    \"\"\"\n","    Applies temperature scaling to the model's internal confidence.\n","    If T > 1, it makes an overconfident model more 'humble'.\n","    \"\"\"\n","    # Simplified Platt Scaling / Temperature Scaling logic\n","    calibrated = np.power(raw_score, 1/temperature)\n","    return min(float(calibrated), 0.99)\n","\n","def get_severity_color(confidence):\n","    if confidence > 0.85: return \"green\"\n","    if confidence > 0.65: return \"orange\"\n","    return \"red\""],"metadata":{"id":"I5EZeZ03ABIz","executionInfo":{"status":"ok","timestamp":1767462940133,"user_tz":-240,"elapsed":2254,"user":{"displayName":"Travour Infinity","userId":"05960736380634090970"}}},"execution_count":26,"outputs":[]},{"cell_type":"code","source":["%%writefile app.py\n","import streamlit as st\n","import time\n","import random\n","\n","# --- Sentinel 10-K Core Logic ---\n","def sentinel_analysis(query):\n","    \"\"\"Simulates a calibrated RAG pipeline result.\"\"\"\n","    # Logic: 2.5 Flash usually overestimates confidence\n","    raw_confidence = random.uniform(0.7, 0.98)\n","\n","    # Apply our Calibration Fitting (T=1.4 for 2.5 Flash)\n","    calibrated_conf = raw_confidence ** (1/1.4)\n","\n","    return {\n","        \"answer\": f\"Analysis for **{query}**: The debt-to-equity ratio has decreased by 12% YoY, primarily due to a $2B bond retirement in Q3. [Source 1]\",\n","        \"confidence\": calibrated_conf,\n","        \"trace_id\": f\"trc_{random.randint(1000, 9999)}\",\n","        \"sources\": [{\"id\": \"SEC-10K-2025-P45\", \"text\": \"Total liabilities decreased from $14B to $12B...\"}]\n","    }\n","\n","# --- UI Setup ---\n","st.set_page_config(page_title=\"Sentinel 10-K\", page_icon=\"ğŸ›¡ï¸\", layout=\"wide\")\n","\n","st.title(\"ğŸ›¡ï¸ Sentinel 10-K Intelligence\")\n","st.caption(\"Verifiable Financial RAG Terminal | Gemini 3 Engine\")\n","\n","# --- Dashboard Layout ---\n","query = st.text_input(\"Financial Research Query\", placeholder=\"e.g. Impact of interest rates on net margin\")\n","\n","if query:\n","    with st.spinner(\"Executing Calibrated Trace...\"):\n","        result = sentinel_analysis(query)\n","        time.sleep(1)\n","\n","    # 1. Main Analysis Result\n","    st.info(f\"**Final Analysis:** {result['answer']}\")\n","\n","    # 2. Confidence & Calibration Metrics\n","    c1, c2, c3 = st.columns(3)\n","\n","    conf_pct = int(result['confidence'] * 100)\n","    c1.metric(\"Calibration Score\", f\"{conf_pct}%\", help=\"The probability that this answer is factually grounded.\")\n","    c2.metric(\"Trace ID\", result['trace_id'])\n","    c3.progress(result['confidence'], text=\"Reliability Gauge\")\n","\n","    # 3. Source Trace (LangSmith-style visibility)\n","    st.divider()\n","    with st.expander(\"ğŸ› ï¸ Reasoning Trace & Chunks\", expanded=False):\n","        st.write(\"### Metadata\")\n","        st.json({\"model\": \"gemini-2.5-flash\", \"temperature_scaling\": 1.4, \"status\": \"verified\"})\n","\n","        st.write(\"### Retrieval Chunks\")\n","        for src in result['sources']:\n","            st.warning(f\"**Source: {src['id']}**\\n\\n{src['text']}\")\n","\n","# --- Evaluation Sidebar ---\n","with st.sidebar:\n","    st.subheader(\"System Health\")\n","    st.success(\"API Status: Connected\")\n","    st.write(\"**Quota Remainder:** 18/20\")\n","\n","    st.divider()\n","    if st.button(\"Download Evaluation Report\"):\n","        st.write(\"Generating Ragas metrics...\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"ZZGeXuh_AIT-","execut